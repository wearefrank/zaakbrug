<Module xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../FrankConfig.xsd">
    <Adapter name="CheckZgwZaakobject"
        active="${CheckZgwZaakobject.Active}"
        description="">

        <Receiver name="CheckZgwZaakobject">
            <JavaListener name="CheckZgwZaakobject"/>
        </Receiver>

        <Pipeline>
            <Exits>
                <Exit name="exists" state="SUCCESS" />
                <Exit name="NOTFOUND" state="SUCCESS" code="404" />
                <Exit name="EXCEPTION" state="ERROR"/>
            </Exits>

            <XsltPipe
                name="SelectZgwZaakobjectsFromListWithMatchingIdentity"
                styleSheetName="Common/xsl/SelectZgwZaakobjectsFromListWithMatchingIdentity.xslt">
                <Param name="MatchWithZgwZaakobject" sessionKey="MatchWithZgwZaakobject" type="DOMDOC" />
                <Param name="debug" value="false" />
                <Forward name="success" path="ZgwZaakobjectMatchesExactlyOneZgwZaakobjectInList" />
                <Forward name="exception" path="EXCEPTION" />
            </XsltPipe>

            <IfPipe
                name="ZgwZaakobjectMatchesExactlyOneZgwZaakobjectInList"
                xpathExpression="count(/ZgwZaakobjecten/*) = 1">
                <Forward name="then" path="exists" />
                <Forward name="else" path="NOTFOUND" />
            </IfPipe>
        </Pipeline>
    </Adapter>
</Module>